#include "pch.h"
#include <iostream>
#include <sstream>
using namespace std;
int main()
{
	int numeros[10], i, max;
	cout << "Por favor ingrese 10 numeros:\n";
	for (i = 0; i < 10; i++)
	{
		cin >> numeros[i];
	}
	max = numeros[0];
	for (i = 0; i < 10; i++)
	{
		if (max < numeros[i])
			max = numeros[i];
	}
	cout << "El numero mas grande es: " << max;
}

